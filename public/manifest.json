{
  "manifest_version": 3,
  "name": "PreFilter.AI",
  "version": "1.0.0",
  "description": "芸能人・インフルエンサーのSNS投稿を投稿前にAIでチェックするサービス",
  "action": {
    "default_popup": "index.html",
    "default_icon": {
      "16": "icons/icon16.png",
      "48": "icons/icon48.png",
      "128": "icons/icon128.png"
    }
  },
  "permissions": [
    "activeTab"
  ],
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  }
}